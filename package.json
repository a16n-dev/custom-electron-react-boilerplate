{
  "name": "manual_setup",
  "version": "1.0.0",
  "description": "Simple setup for electron + react",
  "main": "app/electron/main.js",
  "scripts": {
    "dev": "concurrently --success first \"npm run dev-server\" \"npm run electron\" -k",
    "pack": "npm run build && npm run eb-pack",
    "dist":"npm run build && npm run eb-dist",
    "electron": "cross-env NODE_ENV=development electron .",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "eb-pack": "electron-builder --dir",
    "eb-dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "com.example.app",
    "files": [
      "app/dist/**/*",
      "app/electron/**/*"
    ]
  },
  "dependencies": {
    "electron-updater": "^4.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "electron": "^10.1.1",
    "electron-builder": "^22.8.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "^4.4.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  }
}